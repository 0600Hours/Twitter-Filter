{
  "name": "Twitter Filter",
  "version": "0.0.3",
  "description": "Filter out obnoxious Twitter posts.",
  "permissions": ["activeTab"],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["http://*.twitter.com/*", "https://*.twitter.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "manifest_version": 2
}
